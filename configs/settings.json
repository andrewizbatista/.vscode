{
  "diffEditor.hideUnchangedRegions.enabled": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.bracketPairColorization.enabled": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports.biome": "explicit"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.detectIndentation": false,
  "editor.fontFamily": "Cascadia Code",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.fontWeight": 400,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": "active",
  "editor.lineHeight": 24,
  "editor.minimap.enabled": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.semanticHighlighting.enabled": false,
  "editor.snippetSuggestions": "inline",
  "editor.suggest.showSnippets": true,
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "emmet.showExpandedAbbreviation": "never",
  "eslint.validate": ["typescript", "typescriptreact"],
  "explorer.autoReveal": false,
  "explorer.compactFolders": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.decorations.badges": true,
  "files.associations": {
    ".commitlintrc": "json",
    ".env.*": "dotenv",
    ".huskyrc": "json",
    ".lintstagedrc": "json",
    ".releaserc": "json",
    ".stylelintrc": "json"
  },
  "files.defaultLanguage": "typescriptreact",
  "files.eol": "\n",
  "git.autofetch": true,
  "git.confirmSync": false,
  "gruvboxMaterial.darkContrast": "medium",
  "gruvboxMaterial.darkPalette": "material",
  "gruvboxMaterial.highContrast": true,
  "gruvboxMaterial.italicComments": false,
  "gruvboxMaterial.italicKeywords": false,
  "javascript.suggest.autoImports": false,
  "javascript.updateImportsOnFileMove.enabled": "never",
  "remote.autoForwardPortsSource": "hybrid",
  "terminal.integrated.fontFamily": "Cascadia Code",
  "terminal.integrated.fontSize": 14,
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "typescript.suggest.autoImports": false,
  "typescript.updateImportsOnFileMove.enabled": "never",
  "window.zoomLevel": 0.5,
  "workbench.colorTheme": "Gruvbox Material Dark",
  "workbench.editor.showTabs": "multiple",
  "workbench.editor.tabSizing": "shrink",
  "workbench.editor.wrapTabs": true,
  "workbench.iconTheme": "file-icons",
  "workbench.settings.editor": "json",
  "workbench.startupEditor": "none"
}

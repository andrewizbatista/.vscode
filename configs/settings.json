{
  "diffEditor.hideUnchangedRegions.enabled": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.bracketPairColorization.enabled": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports.biome": "explicit"
  },
  "editor.defaultFormatter": "biomejs.biome",
  "editor.detectIndentation": false,
  "editor.fontFamily": "Cascadia Code",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.fontWeight": 400,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": "active",
  "editor.lineHeight": 24,
  "editor.minimap.enabled": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.semanticHighlighting.enabled": false,
  "editor.snippetSuggestions": "bottom",
  "editor.suggest.showSnippets": false,
  "editor.suggest.showWords": false,
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "eslint.validate": [
    "typescript",
    "typescriptreact"
  ],
  "explorer.autoReveal": false,
  "explorer.compactFolders": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.decorations.badges": true,
  "files.associations": {
    ".commitlintrc": "json",
    ".env": "dotenv",
    ".env.*": "dotenv",
    ".huskyrc": "json",
    ".lintstagedrc": "json",
    ".releaserc": "json",
    ".stylelintrc": "json",
    "*.css": "tailwindcss"
  },
  "files.defaultLanguage": "typescriptreact",
  "files.eol": "\n",
  "files.exclude": {
    "**/node_modules": true
  },
  "git.autofetch": true,
  "git.confirmSync": false,
  "gruvboxMaterial.darkContrast": "medium",
  "gruvboxMaterial.darkPalette": "material",
  "gruvboxMaterial.highContrast": true,
  "gruvboxMaterial.italicComments": false,
  "gruvboxMaterial.italicKeywords": false,
  "javascript.preferences.importModuleSpecifier": "project-relative",
  "javascript.suggest.autoImports": true,
  "javascript.updateImportsOnFileMove.enabled": "never",
  "remote.autoForwardPortsSource": "hybrid",
  "search.exclude": {
    "**/node_modules": true
  },
  "terminal.integrated.fontFamily": "Cascadia Code",
  "terminal.integrated.fontSize": 14,
  "typescript.preferences.importModuleSpecifier": "project-relative",
  "typescript.suggest.autoImports": true,
  "typescript.updateImportsOnFileMove.enabled": "never",
  "window.zoomLevel": 1,
  "workbench.colorTheme": "Beyond the Horizon",
  "workbench.editor.showTabs": "multiple",
  "workbench.editor.tabSizing": "shrink",
  "workbench.editor.wrapTabs": true,
  "workbench.iconTheme": "bearded-icons",
  "workbench.settings.editor": "json",
  "workbench.startupEditor": "none"
}